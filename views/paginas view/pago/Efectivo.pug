extends Pago

block efectivo 
  .card
    .card-content
      form.row(action="#", method="post")
        .input-field.col.l5
          input.validate#Vivienda(type="text", name="")
          label(for="Vivienda") Nombre completo
        .input-field.col.l5 
          input.validate#DireccionDe(type="text", name="")
          label(for="DireccionDe") Numero de documento
        .input-field.col.l10
          input.validate#DireccionDe(type="text", name="")
          label(for="DireccionDe") Correo Electronico
          .modal-footer 
  .card-content.row
    .col.l2.lef
      img(src="/img/icons/efecty.png", alt="metod1") 
      a(href="#Enviar").waves-effect.waves-green.btn.green.accent-4.modal-trigger Seleccionar metodo
    .col.l2.right
        img(src="/img/icons/baloto.png", alt="metod2") 
        a(href="#Envia").waves-effect.waves-green.btn.green.accent-4.modal-trigger Seleccionar metodo
        #Envia.modal
          .modal-content
            h4.center-align.green-text Se ha guardado con exito.   